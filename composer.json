{
  "name": "aliromero/perfect-money-api",
  "description": "This package lets you easily integrate PerfectMoney API into your PHP application.",
  "license": "MIT",
  "keywords": ["PHP", "PerfectMoney"],
  "authors": [
    {
      "name": "Ali Romero",
      "email": "ali.rom3ro@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": "5.*|6.*|7.*|8.*|9.*"
  },
  "autoload": {
    "psr-4": {
      "Llabbasmkhll\\LaravelZibal\\": "src"
    }
  },
  "require-dev": {
    "orchestra/testbench": "6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Romero\\PerfectMoney\\PMServiceProvider"
      ],
      "aliases": {
        "PerfectMoney": "Romero\\PerfectMoney\\Facades\\PerfectMoney"
      }
    }
  }
}